{
  "compilerOptions": {
    "module": "amd",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "outDir": "public/lib",
    "sourceMap": true,
    "target": "es2017",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": false
  },
  "compileOnSave": true,
  "include": ["websrc", "commonsrc"]
}
